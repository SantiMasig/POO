package nota;

public class Nota {

	private int nota;

	public Nota(int valorInicial) {
		this.nota = valorInicial;
	}
	
	public void setValorNota (int valorNota) {
		
	}

	public int getValor() {
		return this.nota;
	}

	public boolean aprobado() {
		return this.nota >= 4;
	}

	public boolean desaprobado() {
		return this.nota < 4;
	}

	public boolean esNotaValida () {
		
	}
	
	

}
